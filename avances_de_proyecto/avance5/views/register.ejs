<body class="row">
       
   <main class="container">
        <h1>Registrar nuevo usuario </h1>
        <form action="/login/register" method="POST" class="box">
            <div class="row">
                <label for="nombre">Nombre </label><input type="text" name="nombre" required>
                <label for="apellidos"><br>Apellidos </label><input type="text" name="apellidos" required>
                <label for="correo"><br> Correo </label><input type="text" name="correo" required>
                <label for="contraseña"><br>Contraseña</label><input type="password" name="contraseña" required>   <br>               
                <label for ="IdRol"><br>Selecciona el rol del nuevo usuario aquí</label>
                <select class="form-select" aria-label="IdRol" name="IdRol">
                    <option value="1">Administrador </option>
                    <option value="2">Usuario</option>
                   </select>
            </div>


            <br><br>



            <button class="btn waves-effect waves-light" type="submit" name="action">Ingresar
            </button>


        </form>
</main> 
</body>

